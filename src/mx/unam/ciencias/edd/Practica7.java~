package mx.unam.ciencias.edd;
import mx.unam.ciencias.proyecto2.*;
import java.io.*;



/**
 * Práctica 7: Montículos mínimos.
 */
public class Practica7 {
    

    public static void main(String[] args) {
    	int i = 0;
	String[] estructuras = {"Lista",
				"ArbolBinario",
				"ArbolRojinegro","ArbolAVL","ArbolBinarioCompleto","ArbolBinarioOrdenado","Pila","Cola","Grafica"};
    	BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
    	String texto,texto2,texto4;
    	texto = texto2 = " ";
    	try{ 
	    texto = br.readLine();
	    texto = texto.trim();
	    if(texto.equals("")){ 
	    	System.err.println("INGRESA UNA CLASE A GRAFICAR");
	    	return;
	    }
	    
	}catch(IOException e){ 
	    System.err.println("ERROR DE LECTURA DE LA PRIMERA LINEA");
	}
	
	
	if(texto.charAt(0)!='#'){ 
	    try{ 
    	  	br.close();
		System.err.println("EL PRIMER CARACTER DEBE SER #");
	    }catch(IOException e){ 
    		System.err.println("ERROR DE LECTURA DE LA PRIMERA LINEA");
	    }
	    
	    
	}else { 
	    texto = texto.replace('#',' ');
	    texto = texto.trim();
	   	
	   	for(String es:estructuras){
			if(es.equals(texto)){ 
				i = -1;
				break;
			 }
		}

		if(i==-1) {	
		    try{
			
			texto2 = br.readLine();
			br.close();

			
		    } catch(IOException e){ 
			System.err.println("ERROR EN LA LECTURA DE LA SGEUNDA LINEA");
		    }
		     }else {
		     	System.err.println("ESTRUCTURA NO VALIDA");
		     }

	    	
	
	}
	
	
	
    }
} 
